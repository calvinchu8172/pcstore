<!DOCTYPE html>
<html>
<head>
  <title>PCstore</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<%= render 'admin_navbar' %>
<div class="container">
  <%= render 'flash'%>
    <div class="row">
      <div class="col-md-3">
        <%= render 'admin_menu' %>
      </div>
      <div class="col-md-9">
      <%= yield %>
      </div>
    </div>
</div>

</body>
</html>
